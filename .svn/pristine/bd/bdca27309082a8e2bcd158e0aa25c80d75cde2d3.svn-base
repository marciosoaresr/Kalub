<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ProfitManager.Fabrica.Entidade.GrupoArea, ProfitManager.Fabrica" table="GrupoArea" lazy="true">
    <cache usage="read-write"/>
    <id name="Id">
      <column name="Id"/>
      <generator class="native">
        <param name="sequence">GrupoArea_</param>
      </generator>
    </id>
    <property name="DataHoraCriacao" column= "DataHoraCriacao" type="Timestamp"/>
    <property name="DataHoraAlteracao" column= "DataHoraAlteracao" type="Timestamp"/>

    <property name="Codigo" column= "Codigo" type="String" length="50"/>
    <property name="Nome" column= "Nome" type="String" length="500"/>
    

  </class>
</hibernate-mapping>