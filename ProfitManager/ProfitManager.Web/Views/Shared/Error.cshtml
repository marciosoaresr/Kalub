@model System.Web.Mvc.HandleErrorInfo
@{
    ViewBag.Title = "KALUB";
    Layout = "~/Views/Shared/_Layout.cshtml";


}

<b>Message:</b> @Model.Exception.Message <br />
<b>Exception:</b> @Model.Exception.GetType().FullName<br />
<b>Controller:</b> @Model.ControllerName<br />
<b>Action:</b> @Model.ActionName


<br/><br/><br/><Br/><br/>
<div align="center">
    <div class="panel " style="padding-bottom: 20px; width: 750px;">
        <div class="panel-body">
            
            <table>
                <tr>
                    <td style="text-align: center">
                        <img src="../../Images/icon-sad.png" width="50" /><br /><br />
                        <span style="font-size: 16.0px">
                            Ops! houve um problema e ja estamos trabalhando para resolver!.<br />
                        </span>
                    </td>
                </tr>
            </table>

        </div>
    </div>
</div>


<div id="abrirModalProcessando" class="modal fade ">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-body" style="height: 500px;">
@if (Model != null)
{
    <div style="margin-top: 20px;">
        <p>
            <b>Message:</b> @Model.Exception.Message <br />
            <b>Exception:</b> @Model.Exception.GetType().FullName<br />
            <b>Controller:</b> @Model.ControllerName<br />
            <b>Action:</b> @Model.ActionName
        </p>
        <div style="overflow: scroll; margin-top: 20px;">
            <p>
                @Model.Exception.StackTrace
            </p>
        </div>
    </div>
}            </div>
        </div>
    </div>
</div>

@section Scripts{
    <script>
        $(document).bind('keydown', function (e) {
            if (e.which == 120) { // 120 = F9
                $('#abrirModalProcessando').modal('show');
                return false;
            }
        });


    </script>

}